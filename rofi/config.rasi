configuration {
	modi: "run,drun,window";
	width: 33;
	lines: 9;
	columns: 1;
	font: "FontAwesome 12";
	location: 2;
	yoffset: 200;
	xoffset: 0;
	fixed-num-lines: false;
	show-icons: true;
	drun-icon-theme: "Newaita-Dark";
	terminal: "kitty";
	run-shell-command: "{terminal} -e {cmd}";
	window-match-fields: "all";
	drun-match-fields: "name,exec";
	case-sensitive: false;
	cycle: true;
	sidebar-mode: true;
	auto-select: false;
	combi-modi: "window,drun";
	matching: "regex";
	hide-scrollbar: true;
	fullscreen: false;
	threads: 0;
	window-format: "{w:0}	{n:0}	{t:0}";
	click-to-exit: true;
	show-match: true;
	max-history-size: 25;
	-hide-mode-prefix: true;
